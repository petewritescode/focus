{
    "name": "Focus",
    "description": "Block distracting sites, stay focused and get more done",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [ "storage" ],
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}